<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.MainActivity"><![CDATA[

    />

    ]]>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/pager"
        android:layout_width="300dp"
        android:layout_height="160dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/introducirNombreEquipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="24dp"
        android:text="Nombre del equipo:"
        app:layout_constraintEnd_toEndOf="@+id/introducirNombreEquipoEscrito"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/introducirNombreEquipoEscrito"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="200dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/introducirNombreSimple"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="28dp"
        android:text="Nombre Simple:"
        app:layout_constraintEnd_toStartOf="@+id/introducirNombreSimpleEscrito"
        app:layout_constraintTop_toBottomOf="@+id/introducirNombreEquipo" />

    <TextView
        android:id="@+id/introducirNombreSimpleEscrito"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="200dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/introducirNombreEquipoEscrito" />

    <TextView
        android:id="@+id/introducirAbreviacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="28dp"
        android:text="Abreviacion:"
        app:layout_constraintEnd_toStartOf="@+id/introducirAbreviacionEscrita"
        app:layout_constraintTop_toBottomOf="@+id/introducirNombreSimple" />

    <TextView
        android:id="@+id/introducirAbreviacionEscrita"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="200dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/introducirNombreSimpleEscrito" />

    <TextView
        android:id="@+id/introducirLocalizacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="28dp"
        android:text="Localizacion:"
        app:layout_constraintEnd_toStartOf="@+id/introducirLocalizacionEscrita"
        app:layout_constraintTop_toBottomOf="@+id/introducirAbreviacion" />

    <TextView
        android:id="@+id/introducirLocalizacionEscrita"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="200dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/introducirAbreviacionEscrita" />

    <Button
        android:id="@+id/boton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginBottom="36dp"
        android:text="Insetar Jugador"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>